{
  "name": "tango-rest-client",
  "version": "1.0.0",
  "description": "JS client for Tango REST API",
  "main": "dist/index.js",
  "source": "index.js",
  "module": "dist/index.js",
  "jest": {
    "preset": "jest-puppeteer"
  },
  "scripts": {
    "build": "microbundle -f es",
    "dev": "microbundle watch -f es",
    "deploy": "curl -T $npm_package_main -H \"Authorization:Basic ${BINTRAY_AUTH_TOKEN}\" https://api.bintray.com/content/waltz-controls/libs/$npm_package_name/$npm_package_version/$npm_package_name/$npm_package_version/index.js",
    "test": "jest-puppeteer ./test/*.js --args no-sandbox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waltz-controls/tango-rest-client.git"
  },
  "keywords": [
    "tango-controls",
    "tango-rest",
    "rest-api",
    "waltz",
    "waltz-controls"
  ],
  "author": "Ingvord",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/waltz-controls/tango-rest-client/issues"
  },
  "homepage": "https://github.com/waltz-controls/tango-rest-client#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.7.4",
    "eventbus": "^1.0.0",
    "jest": "^24.9.0",
    "jest-puppeteer": "^4.3.0",
    "microbundle": "^0.11.0",
    "puppeteer": "^2.0.0",
    "webix": "^7.0.0"
  }
}
